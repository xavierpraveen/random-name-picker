
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model User {
  id        String     @id @default(auto()) @map("_id") @db.ObjectId  
  userId    String        @unique()
  userName  String
  sex       String
  level     String    @default("Beginner")
  status    Int       @default(0)
}
